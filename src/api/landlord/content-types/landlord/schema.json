{
  "kind": "collectionType",
  "collectionName": "landlords",
  "info": {
    "singularName": "landlord",
    "pluralName": "landlords",
    "displayName": "Landlord"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "LndCode": {
      "type": "string",
      "required": true,
      "unique": true,
      "regex": "^LND-[0-9]{7}$"
    },
    "lndErpId": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "lndNameEn": {
      "type": "string",
      "required": true
    },
    "lndNameAr": {
      "type": "string",
      "required": true
    },
    "lndUaeIdNumber": {
      "type": "string",
      "maxLength": 15,
      "required": true,
      "unique": true
    },
    "lndFtaNumber": {
      "type": "string",
      "required": false,
      "unique": true
    },
    "recordStatus": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::record-status.record-status",
      "inversedBy": "landlords"
    },
    "buildings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::building.building",
      "mappedBy": "landlord"
    }
  }
}
