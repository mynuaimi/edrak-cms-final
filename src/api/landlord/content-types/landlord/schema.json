{
  "kind": "collectionType",
  "collectionName": "landlords",
  "info": {
    "singularName": "landlord",
    "pluralName": "landlords",
    "displayName": "Landlord"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "lndCode": {
      "type": "string",
      "required": true,
      "unique": true,
      "regex": "^LND-[0-9]{7}$"
    },
    "lndErpId": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "lndNameEn": {
      "type": "string",
      "required": true
    },
    "lndNameAr": {
      "type": "string",
      "required": true
    },
    "lndUaeIdNumber": {
      "type": "string",
      "maxLength": 15,
      "required": true,
      "unique": true
    },
    "lndFtaNumber": {
      "type": "string",
      "required": false,
      "unique": true
    },
    "buildings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::building.building",
      "mappedBy": "landlord"
    },
    "lndRecordStatus": {
      "type": "enumeration",
      "enum": [
        "Active",
        "Inactive"
      ]
    },
    "mstr_landlord_secondary": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::mstr-landlord-secondary.mstr-landlord-secondary",
      "mappedBy": "landlord"
    }
  }
}
