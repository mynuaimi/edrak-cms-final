{
  "kind": "collectionType",
  "collectionName": "mstr_units",
  "info": {
    "singularName": "mstr-unit",
    "pluralName": "mstr-units",
    "displayName": "Mstr Unit"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "unitCode": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "unitName": {
      "type": "string",
      "required": true
    },
    "building": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::building.building",
      "inversedBy": "mstr_units"
    },
    "ref_leasing_status": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::ref-leasing-status.ref-leasing-status",
      "inversedBy": "mstr_units"
    },
    "mstr_unit_apartment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::mstr-unit-apartment.mstr-unit-apartment",
      "inversedBy": "unit"
    },
    "unitErpId": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "unitFloorId": {
      "type": "string"
    },
    "unitFloor": {
      "type": "string"
    },
    "ref_unit_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::ref-unit-category.ref-unit-category",
      "inversedBy": "mstr_units"
    },
    "unitRecordStatus": {
      "type": "enumeration",
      "enum": [
        "Active",
        "Inactive"
      ]
    }
  }
}
